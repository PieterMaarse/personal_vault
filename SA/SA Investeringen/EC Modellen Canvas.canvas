		{"id":"0ab494b6458d05a7","type":"text","text":"","x":1065,"y":-180,"width":399,"height":60},
		{"id":"67d41c45af12b817","type":"text","text":"Meenemen van submodules?\n- e.g. voor financiële fircties\n- Imperfecte en assymmetrisch informatie (p67)","x":-280,"y":2820,"width":411,"height":151},
		{"id":"b592b8f3b44d1e25","type":"text","text":"","x":171,"y":2820,"width":405,"height":150},
		{"id":"f372589f0e994dac","type":"text","text":"Submodules: \n- NEEM voor environmental dingen","x":616,"y":2820,"width":405,"height":150},
		{"id":"92786deb4fb818ce","type":"text","text":"","x":1061,"y":2820,"width":395,"height":150},
